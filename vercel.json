{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"    
        }
    ],
    "routes": [
        {
            "src": "/sign-in/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/sign-on/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/log/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "http://localhost:5173/",
              "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
              "Access-Control-Allow-Headers": "Content-Type"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}