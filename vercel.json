{
    "version": 3,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"    
        }
    ],
    "routes": [
        {
            "src": "/sign-in/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/sign-on/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}