{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"    
        }
    ],
    "routes": [
        {
            "src": "/sign-in/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/sign-on/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/log/(.*)",
            "methods": ["POST"],
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}